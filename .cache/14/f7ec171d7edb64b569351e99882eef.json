ed <plus> | 1 <minus> | 2 <multiply> | 3 <divide> | 4 <combine> | 5 <modulo> | 6 <exponential> | 7 <percentage>\r\n    if?: string; // \">\" | \"=\"\r\n    bang?: string; // TronicTwitch set bang command\r\n\r\n    constructor () {\r\n        this.name = this.constructor.name;\r\n        this.position = new Position;\r\n    }\r\n\r\n    // TODO Add method to change position\r\n}"},"lineCount":null}},"error":null,"hash":"70792fc6d7b8dcb8cbc78cdb469ef009","cacheData":{"env":{}}}